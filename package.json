{
  "name": "@ladjs/assets",
  "description": "Assets for Lad",
  "version": "1.4.2",
  "author": "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)",
  "bugs": {
    "url": "https://github.com/ladjs/assets/issues",
    "email": "niftylettuce@gmail.com"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "contributors": [
    "Nick Baugh <niftylettuce@gmail.com> (http://niftylettuce.com/)"
  ],
  "dependencies": {
    "clipboard": "^2.0.10",
    "is-string-and-not-blank": "^0.0.2",
    "qs": "^6.10.3",
    "spinkit": "1.x",
    "superagent": "^7.0.2",
    "sweetalert2": "8.x",
    "url-parse": "^1.5.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@ladjs/browserslist-config": "^0.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-config-xo-lass": "^1.0.6",
    "fixpack": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "remark-cli": "^10.0.1",
    "remark-preset-github": "^4.0.1",
    "xo": "^0.47.0"
  },
  "engines": {
    "node": ">=8.3"
  },
  "files": [
    "lib",
    "scss"
  ],
  "homepage": "https://github.com/ladjs/assets",
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "4",
    "ajax",
    "assets",
    "auto",
    "beta",
    "bootstrap",
    "clipboard",
    "confirm",
    "custom",
    "file",
    "fix",
    "form",
    "frontend",
    "hash",
    "input",
    "jump",
    "lad",
    "lass",
    "on",
    "prompt",
    "return",
    "scroll"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "preset-github"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ladjs/assets"
  },
  "scripts": {
    "build": "babel src --out-dir lib --source-maps",
    "lint": "xo && remark . -qfo",
    "test": "npm run lint && npm run build",
    "watch": "babel src --watch --out-dir lib --source-maps"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ],
    "envs": [
      "node",
      "browser",
      "jquery"
    ],
    "rules": {
      "unicorn/catch-error-name": "off",
      "import/extensions": "warn",
      "unicorn/prefer-module": "off"
    }
  }
}
